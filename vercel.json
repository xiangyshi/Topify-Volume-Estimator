{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    { "src": "static/**", "use": "@vercel/static" },
    { "src": "data/api_responses/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/data/api_responses/(.*)", "dest": "/data/api_responses/$1" },
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/analyze", "dest": "/api/index.py" },
    { "src": "/analyze-sample", "dest": "/api/index.py" },
    { "src": "/api/check-data", "dest": "/api/index.py" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}